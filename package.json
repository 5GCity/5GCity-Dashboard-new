{
  "name": "5GCity-frontend",
  "version": "1.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://gitlab.ubiwhere.com/knowledge-base/frontend/Starters/ReactKea.git"
  },
  "dependencies": {
    "axios": "^0.17.0",
    "element-react": "^1.4.24",
    "element-theme-default": "^1.4.13",
    "express": "^4.16.2",
    "history": "^4.7.2",
    "jsdom": "^11.11.0",
    "kea": "^0.28.3",
    "kea-saga": "^0.3.4",
    "keycloak-js": "^4.5.0",
    "lodash": "^4.17.4",
    "mapbox-gl": "^0.51.0",
    "moment": "^2.19.2",
    "object-assign": "4.1.1",
    "path": "^0.12.7",
    "path-to-regexp": "^2.1.0",
    "polished": "^1.9.3",
    "promise": "8.0.1",
    "prop-types": "^15.6.0",
    "raven-js": "^3.20.1",
    "react": "^16.6.1",
    "react-dev-utils": "^4.1.0",
    "react-dimensions": "^1.3.1",
    "react-dom": "^16.0.0",
    "react-frontload": "^1.0.1",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^3.1.1",
    "react-loadable": "^5.4.0",
    "react-map-gl": "3.0.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "react-test-renderer": "^16.0.0",
    "recompose": "^0.26.0",
    "redux": "^3.0.0",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "styled-components": "^2.2.1"
  },
  "scripts": {
    "setup": "node scripts/setup.js",
    "start": "cross-env APP_ENV=development node scripts/start.js",
    "start:staging": "cross-env APP_ENV=staging node scripts/start.js",
    "start:production": "cross-env APP_ENV=production node scripts/start.js",
    "build": "cross-env APP_ENV=development node scripts/build.js",
    "build:staging": "cross-env APP_ENV=staging node scripts/build.js",
    "build:production": "cross-env APP_ENV=production node scripts/build.js",
    "start:server": "cross-env NODE_ENV=production node server/index.js --env=jsdom",
    "test": "node scripts/test.js --env=jsdom --setupTestFrameworkScriptFile=raf/polyfill --coverage",
    "test:watch": "node scripts/test.js --env=jsdom --setupTestFrameworkScriptFile=raf/polyfill --watch --coverage",
    "test:e2e": "yarn nightwatch",
    "lint": "standard 'src/**/*.js' --verbose | snazzy",
    "lint:fix": "standard 'src/**/*.js' --fix | snazzy",
    "start:storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "generate:container": "cross-env HYGEN_TMPLS=.templates/ hygen container new",
    "generate:component": "cross-env HYGEN_TMPLS=.templates/ hygen component new",
    "generate:scene": "cross-env HYGEN_TMPLS=.templates/ hygen scene new",
    "commitmsg": "commitlint -E GIT_PARAMS",
    "precommit": "lint-staged",
    "release": "semantic-release --no-verify"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/stories.js",
      "!src/registerServiceWorker.js",
      "!src/environments/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\](?!ol).*\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "moduleDirectories": [
      "src",
      "node_modules"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "transform-class-properties",
      "react-hot-loader/babel",
      "transform-decorators-legacy",
      "lodash",
      "babel-plugin-styled-components"
    ]
  },
  "standard": {
    "ignore": [
      "registerServiceWorker.js",
      "**/test.js",
      "**/logic.test.js",
      "**/*.test.js"
    ],
    "parser": "babel-eslint"
  },
  "lint-staged": {
    "src/**/*.js": [
      "yarn test",
      "yarn lint:fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@commitlint/prompt-cli": "^7.0.0",
    "@semantic-release/changelog": "^3.0.0",
    "@semantic-release/git": "^7.0.1",
    "@semantic-release/gitlab-config": "^3.0.0",
    "@storybook/addon-actions": "^3.2.13",
    "@storybook/addon-links": "^3.2.13",
    "@storybook/react": "^3.2.13",
    "autoprefixer": "7.1.2",
    "babel-core": "6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-plugin-dynamic-import-node": "^2.0.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react-app": "^3.0.3",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "changelog.md": "^1.1.0",
    "confusing-browser-globals": "^0.0.0",
    "conventional-changelog-cli": "^2.0.1",
    "conventional-recommended-bump": "^4.0.0",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.2.0",
    "css-loader": "0.28.4",
    "depcheck": "^0.6.8",
    "dotenv": "4.0.0",
    "enzyme": "^3.1.0",
    "eslint": "4.4.1",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-class-property": "^1.0.6",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "faker": "^4.1.0",
    "file-loader": "0.11.2",
    "flow-bin": "^0.65.0",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "humps": "^2.0.1",
    "husky": "^0.14.3",
    "hygen": "^1.4.1",
    "ignore-styles": "^5.0.1",
    "inquirer": "^6.0.0",
    "jest": "20.0.4",
    "jest-styled-components": "^5.0.1",
    "lint-staged": "^7.2.0",
    "md5-file": "^4.0.0",
    "morgan": "^1.9.0",
    "nightwatch": "^0.9.21",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "replace": "^1.0.0",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.8.1",
    "slugify": "^1.3.0",
    "standard": "^10.0.3",
    "standard-loader": "^6.0.1",
    "standard-version": "^4.4.0",
    "style-loader": "0.18.2",
    "svg-react-loader": "next",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.5.9",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.8.2",
    "webpack-manifest-plugin": "1.2.1",
    "whatwg-fetch": "2.0.3"
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/gitlab"
    ],
    "prepare": [
      {
        "path": "@semantic-release/changelog",
        "changelogFile": "CHANGELOG.md"
      },
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "CHANGELOG.md"
        ],
        "message": "chore(release): ${nextRelease.version} \n\n${nextRelease.notes}"
      }
    ],
    "publish": [
      "@semantic-release/npm",
      "@semantic-release/gitlab"
    ],
    "success": false,
    "fail": false,
    "gitlabUrl": "https://gitlab.ubiwhere.com",
    "npmPublish": false,
    "tarballDir": false
  }
}
